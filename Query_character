def query_character(name: str = None, 
                   page: int = None, 
                   id: int = None, 
                   status: str =  None, 
                   species: str = None, 
                   type: str = None, 
                   gender: str = None, 
                   origin: Tuple[str] = None, 
                   location = None, 
                   image: str = None, 
                   url: List[str] = None, 
                   create: str = None)->dict:
    
    """
    Arguments: 
        status (str): The status of the character ('Alive', 'Dead' or 'unknown').
        gender (str): The gender of the character ('Female', 'Male', 'Genderless' or 'unknown').
    Returns: 
        dict: response object converted to dict
    
    Note: 
        - Find more information here: https://rickandmortyapi.com/documentation#episode
    """
    
    
    payload = {
        "name" : name, 
        "page" : page,
        "id" : id, 
        "status" : status ,
        "species" : species,
        "type" : type , 
        "gender" : gender , 
        "origin" : origin, 
        "location" : location , 
        "image" : image , 
        "url" : url , 
        "create" : create,
    }
    
    resp_json = requests.get(characterURL, params = payload).json()
    resp = requests.get(characterURL, params = payload)
    
    if resp.status_code == 200: 
        return resp_json
    else:
        print(resp.status_code) 
        print(resp.text)
        
